{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "react"
  ],
  "rules": {
    "camelcase": 0,
    "no-param-reassign": ["error", { "props": false }],
    "import/prefer-default-export": 0,
    "space-before-function-paren": ["error", {
      "anonymous": "ignore",
      "named": "ignore",
      "asyncArrow": "ignore"
    }],
    "object-shorthand": [0, "consistent"],
    "no-warning-comments": 0,
    "react/jsx-uses-vars": [2],
    "class-methods-use-this": 0
  }
}
